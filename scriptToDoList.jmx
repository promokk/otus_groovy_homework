              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import java.time.LocalDateTime
import java.time.format.DateTimeFormatter
import groovy.time.TimeCategory

//Генерация параметров старта и конца задачи / действия

LocalDateTime currentDate = LocalDateTime.now()
DateTimeFormatter formatter = DateTimeFormatter.ofPattern(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss&quot;)

def timeTaskStart = currentDate.format(formatter)
def timeTaskEnd = currentDate.plusSeconds(3).format(formatter)

def timeActionStart = currentDate.plusSeconds(1).format(formatter)
def timeActionEnd = currentDate.plusSeconds(2).format(formatter)

vars.put(&quot;timeTaskStart&quot;, timeTaskStart.toString())
vars.put(&quot;timeTaskEnd&quot;, timeTaskEnd.toString())

vars.put(&quot;timeActionStart&quot;, timeActionStart.toString())
vars.put(&quot;timeActionEnd&quot;, timeActionEnd.toString())
</stringProp>
